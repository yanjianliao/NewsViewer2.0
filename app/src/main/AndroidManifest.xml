<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yanjianliao.newsviewer2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".news_detail.DetailActivity" />
        <activity android:name=".local.LocalNewsActivity" />
        <activity android:name=".news_list.NewsListActivity" />
        <activity android:name=".WebViewActivity" />
        <activity android:name=".NewsCategoryActivity" />
        <activity android:name=".local.CommentActivity"></activity>
    </application>

</manifest>